<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bai">
    <include layout="@layout/title_bar"></include>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="法 人 名 称:" />

                <EditText
                    android:id="@+id/et_organization_code"
                    style="@style/et_style"
                    android:hint="请输入真实姓名"
                    />
            </LinearLayout>

            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="联 系 电 话:" />

                <EditText
                    android:id="@+id/et_enterpeise_name"
                    style="@style/et_style"
                    android:hint="请输入现用手机号"
                    android:inputType="number"
                  />
            </LinearLayout>

            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="税  号:" />

                <EditText
                    android:id="@+id/et_legal_person_name"
                    style="@style/et_style"
                    android:hint="请输入公司税号"
                    android:inputType="number"
                    />
            </LinearLayout>

            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="联 系 地 址:" />

                <EditText
                    android:id="@+id/et_legal_person_icard"
                    style="@style/et_style"
                    android:hint="请输入公司地址"
                   />
            </LinearLayout>

            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="所 属 年 度:" />

                <EditText
                    android:id="@+id/et_provinces"
                    style="@style/et_style"
                    android:hint="请填写所属年度"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="上季度销售额:" />

                <EditText
                    android:id="@+id/et_industry"
                    style="@style/et_style"
                    android:hint="  元"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="年 度 电 费:" />

                <EditText
                    android:id="@+id/et_contact_phone_number"
                    style="@style/et_style"
                    android:hint="  元"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="年度人均总值:" />

                <EditText
                    android:id="@+id/et_the_registered_capital"
                    style="@style/et_style"
                    android:hint="  元"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="现有人员总数:" />

                <EditText
                    android:id="@+id/et_email"
                    style="@style/et_style"
                    android:hint="请输入公司人员总数"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="利 润 率:" />

                <EditText
                    android:id="@+id/et_qq"
                    style="@style/et_style"
                    android:hint="  %"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="总 收 入:" />

                <EditText
                    android:id="@+id/et_wetchcat"
                    style="@style/et_style"
                    android:hint="  元"
                    android:inputType="number"/>
            </LinearLayout>
            <LinearLayout style="@style/line_layout">

                <TextView
                    style="@style/tv_style"
                    android:text="总 投 资:" />

                <EditText
                    android:id="@+id/total_investment"
                    style="@style/et_style"
                    android:hint="  元"
                    android:inputType="number"
                   />
            </LinearLayout>
            <Button
                android:id="@+id/save"
                android:layout_width="285dp"
                android:layout_height="33dp"
                android:background="@drawable/login_btn"
                android:layout_gravity="center_horizontal"
                android:text="保  存"
                android:gravity="center"
                android:textColor="@color/white"
                android:textSize="13sp"
                android:layout_marginTop="30dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"></LinearLayout>
        </LinearLayout>
    </ScrollView>


</LinearLayout>